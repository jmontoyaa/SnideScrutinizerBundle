{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% if collector.data %}
            {% set icon %}
            <span style="line-height:28px;"><img alt="Scrutinizer" src="data:image/png;base64,"/></span>
            <span class="sf-toolbar-status sf-toolbar-status-{{ snide_scrutinizer_color(collector.data.metrics.quality) }}"></span>
            {% endset %}
            {% set text %}
            <div class="sf-toolbar-info-piece">
                <b>Quality</b>
                <span class="sf-toolbar-status-{{ snide_scrutinizer_color(collector.data.metrics.quality) }}">{{ collector.data.metrics.quality }} / 10</span>
            </div>
            <div class="sf-toolbar-info-piece">
                <b>Coverage</b>
                <span class="sf-toolbar-status sf-toolbar-status-{{ snide_scrutinizer_color(collector.data.metrics.testCoverage * 10) }}">{{ collector.data.metrics.testCoverage|number_format(2) * 100}}%</span>
            </div>
            {% endset %}
        {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
    {% endif %}
{% endblock %}

{% block menu %}
    <span class="label">
    <span class="icon"><img width="32" height="32" alt="scrutinizer" src="data:image/png;base64,"/></span>
    <strong>Scrutinizer</strong>
    </span>

</span>
{% endblock %}

{% block panel %}
    <h2>Scrutinizer</h2>
    {% if not collector.data %}
        <div>
            <h3>No data available</h3>
            The client could not connect to the web service. Maybe you are offline?
        </div>
    {% else %}
        Hello world
    {% endif %}
{% endblock %}