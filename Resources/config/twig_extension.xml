<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="snide_scrutinizer.twig.scrutinizer_extension.class">Snide\Bundle\ScrutinizerBundle\Twig\Extension\ScrutinizerExtension</parameter>
    </parameters>

    <services>
        <service id="snide_scrutinizer.twig.scrutinizer_extension" class="%snide_scrutinizer.twig.scrutinizer_extension.class%">
            <tag name="twig.extension" />
        </service>

    </services>
</container>